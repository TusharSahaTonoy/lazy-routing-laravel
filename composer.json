{
    "name": "tushar/lazy-routing",
    "description": "This is a Laravel library package once install you don't need to declare a route for each method instead you follow a normal camel case convention to declare the method name and the route will auto declared for you.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tonoysh",
            "email": "tonoysh@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "package",
        "lazy",
        "routing",
        "route",
        "auto",
        "generation"
    ],
    "homepage": "https://github.com/TusharSahaTonoy/lazy-routing-laravel",
    "autoload": {
        "psr-4": {
            "Tushar\\LazyRouting\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {},
    "extra": {
        "laravel": {
            "providers": [
                "Tushar\\LazyRouting\\LazyServiceProvider"
            ]
        }
    }
}
